{
  "name": "gastroflow-proto",
  "version": "1.0.0",
  "description": "Generated TypeScript/JavaScript code for GastroFlow Protocol Buffers. Provides strongly-typed models and gRPC clients for restaurant management systems.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "protobuf",
    "grpc",
    "restaurant",
    "gastroflow",
    "api",
    "client",
    "typescript",
    "javascript",
    "food",
    "orders",
    "menu",
    "payments",
    "pos"
  ],
  "author": {
    "name": "GastroFlow Team",
    "email": "dev@gastroflow.com",
    "url": "https://gastroflow.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gastroflow/gastroflow-proto.git"
  },
  "bugs": {
    "url": "https://github.com/gastroflow/gastroflow-proto/issues"
  },
  "homepage": "https://proto.gastroflow.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.4",
    "@types/google-protobuf": "^3.15.12",
    "google-protobuf": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.67",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/gastroflow"
  }
}